{% extends 'base.html.twig' %}

{% block title %}Yeast{% endblock %}

{% block body %}
    <h1>Yeast</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ yeast.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ yeast.name }}</td>
            </tr>
            <tr>
                <th>Flocculation</th>
                <td>{{ yeast.flocculation }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ yeast.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_yeast_index') }}">back to list</a>

    <a href="{{ path('app_yeast_edit', {'id': yeast.id}) }}">edit</a>

    {{ include('yeast/_delete_form.html.twig') }}
{% endblock %}
